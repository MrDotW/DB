{
    "background": {
        "persistent": false,
        "scripts": [
            "ext.js",
            "bg.js"
        ]
    },
    "content_scripts": [
        {
            "exclude_matches": [],
            "matches": [
                "*://weibo.cn/*",
                "*://m.weibo.cn/*"
            ],
            "js": [
                "ext.js",
                "inject.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "https://www.douban.com/people/*/statuses*"
            ],
            "js": [
                "ext.js",
                "inject2.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "description": "Backup online life.",
    "icons": {
        "128": "/img/icn-64.png",
        "16": "/img/icn-64.png",
        "48": "/img/icn-64.png"
    },
    "homepage_url": "https://github.com/MrDotW/DB",
    "author": "weiiihaooo",
    "options_page": "opt.html",
    "manifest_version": 2,
    "minimum_chrome_version": "54",
    "name": "Feedly Notification",
    "permissions": [
        "*://*.weibo.cn/*",
        "https://www.douban.com/people/*",
        "cookies"
    ],
    "version": "0.1"
}